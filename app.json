{
  "expo": {
    "name": "SimBaseBall",
    "slug": "SimBaseBall-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.hasuximoto.simbaseball",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hasuximoto.simbaseball"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "lang": "ja"
    },
    "experiments": {
      "baseUrl": "/penanto_baseball_game/"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "f0b724dd-43eb-4696-93f9-de635a4eeec8"
      }
    }
  }
}
